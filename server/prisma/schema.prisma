generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./rgbColorCodesSmall.db"
}

model rgbColorCodesSmall {
  id             Int   @default(autoincrement()) @id
  red            Int
  green          Int
  blue           Int
  contrast_ratio Float
  hue            Int?
  chroma         Int
}
